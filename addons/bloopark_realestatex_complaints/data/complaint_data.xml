<odoo>
    <data>
        <record id="stage_new" model="realestatex.complaint.stage">
            <field name="name">New</field>
            <field name="sequence">1</field>
            <field name="fold">False</field>
        </record>
        <record id="stage_in_review" model="realestatex.complaint.stage">
            <field name="name">In Review</field>
            <field name="sequence">2</field>
            <field name="fold">False</field>
        </record>
        <record id="stage_in_progress" model="realestatex.complaint.stage">
            <field name="name">In Progress</field>
            <field name="sequence">3</field>
            <field name="fold">False</field>
        </record>
        <record id="stage_solved" model="realestatex.complaint.stage">
            <field name="name">Solved</field>
            <field name="sequence">4</field>
            <field name="fold">True</field>
        </record>
        <record id="stage_dropped" model="realestatex.complaint.stage">
            <field name="name">Dropped</field>
            <field name="sequence">5</field>
            <field name="fold">True</field>
        </record>
    </data>
</odoo>
